// webserver -> remote interpreter
protocol s2r 1000 json
{
	Welcome(string msg);
	AckLogin(string id, int result);
	ReqVisualProgRun(webvprog::sNodeFile nodeFile);
//	ReqEvent(string eventName);
}


// remote interpreter -> webserver
protocol r2s 2000 json
{
	ReqLogin(string id);
	AckVisualProgRun(int result);
//	AckRun(int result, script::cIntermediateCode icode);
//	AckEvent(string eventName, int result);
//	SyncRegister( int vmIdx, webvprog::sRegister reg );
}
